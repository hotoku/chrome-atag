{
  "name": "confluence-atag",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "add target='_blank' to all a-tag in confluence pages",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://jdsc.atlassian.net/*"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": ["<all_urls>"]
}
